// Builtin Sorts in C#

using System;
using System.Collections.Generic;
using System.Linq;

class BuiltInSort
{
    public static void Main(string[] args)
    {
        int[] a = new int[7] {1,23,12,45,12,67,2};
        Array.Sort(a);
        foreach(int i in a)
        {
            Console.Write(i + " ");
        }
        
        Console.WriteLine("");
        
    }
}
